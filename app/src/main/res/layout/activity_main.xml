<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="patient.yilin.com.testmd.MainActivity">

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            >
            <Button
                android:id="@+id/btn"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="recy+fab"
                />
            <Button
                android:id="@+id/btn1"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="平行空间启动页效果"
                />
            <Button
                android:id="@+id/btn2"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="coolpasingtoolbarlayout+toolbarlayout"
                />
            <Button
                android:id="@+id/btn3"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="tablayou+viewpager+Appbarlayout"
                />
            <Button
                android:id="@+id/btn4"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="自定义behavior1"
                />
            <Button
                android:id="@+id/btn5"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="自定义behavior2"
                />
            <Button
                android:id="@+id/btn6"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="测试动画效果1"
                />
            <Button
                android:id="@+id/btn7"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="测试MD动画效果"
                />
            <Button
                android:id="@+id/btn8"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="属性动画 支付宝+safari效果"
                />
            <Button
                android:id="@+id/btn9"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="观察者 订阅者简单示例"
                />
            <Button
                android:id="@+id/btn10"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="MD专场动画"
                />
            <Button
                android:id="@+id/btn11"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="MD转场动画+ToolBar"
                />
            <Button
                android:id="@+id/btn12"
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="SVG测试使用"
                />
        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>

</LinearLayout>
